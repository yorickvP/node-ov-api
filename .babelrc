{
    "presets": [
        ["env", {
          "targets": {
            "node": 6.3
          }
        }]
    ],
    "plugins": [
        "transform-async-to-generator",
        "transform-strict-mode", [
            "transform-runtime", {
                "polyfill": false,
                "regenerator": true
            }
        ],
    ]
}